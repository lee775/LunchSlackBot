# Slack Bot Token (starts with xoxb-)
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token

# Slack Channel IDs
# 시작 알림을 받을 채널 (봇 관리용)
SLACK_STARTUP_CHANNEL_ID=C1234567890

# 점심메뉴를 받을 채널 (메인 점심메뉴 채널)
SLACK_LUNCH_CHANNEL_ID=C0987654321

# 기존 호환성을 위한 기본 채널
SLACK_CHANNEL_ID=C1234567890

# KakaoTalk Plus Friend URL (e.g., https://pf.kakao.com/_your_friend_id)
KAKAO_PLUS_FRIEND_URL=https://pf.kakao.com/_your_friend_id

# Cron schedule (default: daily at 9:00 AM)
SCHEDULE_CRON=0 9 * * *

# Log level (error, warn, info, debug)
LOG_LEVEL=info